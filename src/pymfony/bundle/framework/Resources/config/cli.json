{
	"parameters": {
		"cli_kernel.class": "pymfony.component.clikernel.CliKernel",
		"cli.controller_resolver.class": "pymfony.component.clikernel.controller.ControllerResolver",
		"cli.router_listener.class": "pymfony.component.clikernel.listener.RouterListener",
		"cli.response_listener.class": "pymfony.component.clikernel.listener.ResponseListener"
		
	},
	"services": {
		"cli.controller_resolver": {
			"class": "%cli.controller_resolver.class%",
			"arguments": [
				"@service_container"
			]
		},
		"cli_kernel": {
			"class": "%cli_kernel.class%",
			"arguments": [
				"@event_dispatcher",
				"@cli.controller_resolver",
				"%kernel.name%",
				"%kernel.version%"
			]
		},
		"cli.router_listener": {
			"class": "%cli.router_listener.class%",
			"tags": [
				{"name": "kernel.event_subscriber"}
			],
			"arguments": [
				"@service_container"
			]
		},
		"cli.response_listener": {
			"class": "%cli.response_listener.class%",
			"tags": [
				{"name": "kernel.event_subscriber"}
			],
			"arguments": [
			]
		}
	}
}
